{
  "name": "gym-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Monorepo for Gym Management System with Admin Dashboard, Mobile App, and Backend API",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{js,jsx,md}\"",
    "dev:admin": "pnpm --filter gym-admin dev",
    "dev:app": "pnpm --filter gymapp start",
    "dev:backend": "pnpm --filter auth-backend dev",
    "build:admin": "pnpm --filter gym-admin build",
    "build:app": "pnpm --filter gymapp build",
    "build:backend": "pnpm --filter auth-backend build",
    "start:admin": "pnpm --filter gym-admin preview",
    "start:app": "pnpm --filter gymapp start",
    "start:backend": "pnpm --filter auth-backend start",
    "type-check": "turbo run type-check",
    "test:coverage": "turbo run test:coverage"
  },
  "devDependencies": {
    "@turbo/gen": "^1.12.4",
    "@types/node": "^20.10.5",
    "prettier": "^3.1.1",
    "turbo": "^1.12.4"
  },
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "dependencies": {
    "@radix-ui/react-select": "^2.2.6",
    "cloudinary-react": "^1.8.1",
    "jwks-rsa": "^3.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.1"
  }
}
